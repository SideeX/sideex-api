{
    "source": {
        "include": ["./src/sideex-api/index.js"],
        "includePattern": ".js$",
        "excludePattern": "{node_modules/|docs}"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "default": {
            "outputSourceFiles": false,
            "staticFiles": {
                "include": [
                    "./static"
                ]
            }
        },
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "recurse": true,
        "destination": "./docs/",
        "template": "./custom-template",
        "tutorials": "./tutorials"
    }

    
}