{
  "name": "sideex",
  "version": "2.3.3",
  "description": "Smart Record-Playback Browser Automation",
  "scripts": {
    "test": "jest --config jest.config.js",
    "fix-cs": "eslint --fix",
    "start": "webpack-dev-server -w --config webpack.ui.config.js --hot --progress --colors",
    "build": "concurrently --kill-others \"npm run build-ts\" \"webpack --config webpack.config.js\"",
    "build-libs": "webpack --config webpack.libs.js",
    "build-prod": "webpack --config webpack.config.prod.js",
    "build-runner": "webpack --config webpack.runner.config.js",
    "build-ts": "npx tsc -w",
    "build-ui": "webpack --config webpack.ui.config.js",
    "build-webdriver": "webpack --config webpack.webdriver.config.js",
    "dev": "concurrently --kill-others \"npm run build\" \"npm run build-webdriver\"",
    "dev:ui": "concurrently --kill-others \"npm run build-libs\" \"npm run start\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/SideeX/sideex.git"
